<%include partials/navbar%>

<script type="text/javascript" src="homeScript.js"></script>



<!-- Modal -->
<div class="modal fade" id=<%= showSurveyPrompt %> tabindex="-1" role="dialog" >
  <div class="modal-dialog modal-dialog-centered" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h3 class="modal-title" id="exampleModalLongTitle">Would you like to rate your Lookup Experience?</h3>
      </div>
      <div class="modal-body">
        <p>here is a</p>
        <p>survery</p>
        <p>about the lookup experience</p>
      </div>
      <div class="modal-footer">
        <a href="/survey"><button type="button" class="btn btn-success" >Yes!</button></a>
        <button type="button" class="btn btn-danger" data-dismiss="modal">No Thanks</button>
      </div>
    </div>
  </div>
</div>





<h2>Fact of the Day</h2>
<h3 id="nameHolder"><%= name %></h3>
<img src="<%= image %>">
<p><br><b>Description:</b> <br><%= description %></p>
<p><b>Location:</b> <br><%= location %></p>
<button id="getfactbutton" class="btn btn-lg btn-primary btn-block" >Get Another Fact</button>





<%include partials/footer%>
